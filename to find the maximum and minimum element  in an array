nums = list(map(int, input("Enter elements separated by spaces: ").split()))

if len(nums) == 0:
    print("Array is empty.")
else:
    max_value = nums[0]
    min_value = nums[0]

    for num in nums:
        if num > max_value:
            max_value = num
        if num < min_value:
            min_value = num

    print("Maximum value:", max_value)
    print("Minimum value:", min_value)
